{
  "name": "angular-beanie",
  "version": "1.1.0",
  "description": "Angular (1.5+) component to effortlessly set the document title",
  "main": "dist/angular-beanie.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "test": "webpack && karma start",
    "test:watch": "karma start --singleRun=false --autoWatch=true --autoWatchInterval=1",
    "test:debug": "karma start --singleRun=false --autoWatch=true --autoWatchInterval=1 --browsers=Chrome",
    "start": "npm-run-all --parallel build:watch test:watch",
    "debug": "npm-run-all --parallel build:watch test:debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aquibm/angular-beanie.git"
  },
  "keywords": [
    "angular",
    "beanie",
    "set",
    "document",
    "title",
    "component",
    "ng"
  ],
  "author": "Aquib Master <aquibmaster@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aquibm/angular-beanie/issues"
  },
  "homepage": "https://github.com/aquibm/angular-beanie#readme",
  "devDependencies": {
    "angular": "^1.6.2",
    "angular-mocks": "^1.6.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-script-launcher": "^1.0.0",
    "karma-systemjs": "^0.16.0",
    "npm-run-all": "^4.0.1",
    "webpack": "^2.2.1"
  }
}
